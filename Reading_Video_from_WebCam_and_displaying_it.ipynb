      "source": [
        "import cv2\n",
        "\n",
        "video = cv2.videoCapture(0)\n",
        "while(True):\n",
        "  ret, frame = video.read()\n",
        "  gray = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)\n",
        "  cv2.imshow('result', gray)\n",
        "  cv2.waitKey(0)\n",
        "  if waitKey(27) == 33\n",
        "  break\n",
        "\n",
        "cv2.destroyAllWindows()\n"
      ],
